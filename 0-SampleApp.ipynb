    "import requests\n",
    "import gradio as gr\n",
    "\n",
    "from rdkit import Chem\n",
    "from rdkit.Chem import Draw\n",
    "\n",
    "invoke_url = \"http://molmim:8000/generate\"\n",
    "\n",
    "smiles_examples = {\n",
    "    \"Lisuride\": \"[H][C@@]12Cc3c[nH]c4cccc(C1=C[C@H](NC(=O)N(CC)CC)CN2C)c34\",\n",
    "    \"Lenalidomide\": \"O=C1NC(=O)CCC1N3C(=O)c2cccc(c2C3)N\",\n",
    "    \"Ensitrelvir\": \"Cn1cnc(CN2C(=O)N(Cc3cc(F)c(F)cc3F)C(=Nc3cc4cn(C)nc4cc3Cl)NC2=O)n1\",\n",
    "    \"Ibuprofen\": \"CC(C)Cc1ccc(C(C)C(=O)O)cc1\",\n",
    "    \"Floxuridine\": \"O=c1[nH]c(=O)n([C@H]2C[C@H](O)[C@@H](CO)O2)cc1F\",\n",
    "    \"Leflunomide\": \"Cc1oncc1C(=O)Nc1ccc(C(F)(F)F)cc1\",\n",
    "    \"Tolnaftate\": \"Cc1cccc(N(C)C(=S)Oc2ccc3ccccc3c2)c1\",\n",
    "    \"Ifenprodil\": \"CC(C(O)c1ccc(O)cc1)N1CCC(Cc2ccccc2)CC1\",\n",
    "    \"Amoxicillin\": \"CC1(C)S[C@@H]2[C@H](NC(=O)[C@H](N)c3ccc(O)cc3)C(=O)N2[C@H]1C(=O)O\",\n",
    "    \"Donepezil\": \"COc1cc2c(cc1OC)C(=O)C(CC1CCN(Cc3ccccc3)CC1)C2\",\n",
    "    \"Lovastatin\": \"CC[C@H](C)C(=O)O[C@H]1C[C@@H](C)C=C2C=C[C@H](C)[C@H](CC[C@@H]3C[C@@H](O)CC(=O)O3)[C@H]21\",\n",
    "    \"Iloperidone\": \"COc1cc(C(C)=O)ccc1OCCCN1CCC(c2noc3cc(F)ccc23)CC1\",\n",
    "    \"Dicloxacillin\": \"Cc1onc(-c2c(Cl)cccc2Cl)c1C(=O)N[C@@H]1C(=O)N2[C@@H]1SC(C)(C)[C@@H]2C(=O)O\",\n",
    "    \"Caffeine\": \"Cn1c(=O)c2c(ncn2C)n(C)c1=O\",\n",
    "    \"Aspirin\": \"CC(=O)OC1=CC=CC=C1C(=O)O\"\n",
    "}\n",
    "\n",
    "def process_request(\n",
    "    smiles_string,\n",
    "    molecules_to_generate,\n",
    "    sampling_algorithm,\n",
    "    sample_radius,\n",
    "    property_to_optimize,\n",
    "    maximize,\n",
    "    similarity_constraint,\n",
    "    particles,\n",
    "    iterations):\n",
    "\n",
    "    headers = {\n",
    "        \"accept\": \"application/json\",\n",
    "        \"content-type\": \"application/json\"\n",
    "    }\n",
    "\n",
    "    if sampling_algorithm == \"CMA-ES Controlled Generation\":\n",
    "        payload = {\n",
    "            \"algorithm\": \"CMA-ES\",\n",
    "            \"smi\": smiles_string,\n",
    "            \"num_molecules\":  molecules_to_generate,\n",
    "            \"property_name\": property_to_optimize,\n",
    "            \"minimize\": not(maximize),\n",
    "            \"min_similarity\": similarity_constraint,\n",
    "            \"particles\": max(molecules_to_generate, particles),\n",
    "            \"iterations\": iterations\n",
    "        }\n",
    "    else:\n",
    "        payload = {\n",
    "            \"algorithm\": \"none\",\n",
    "            \"smi\": smiles_string,\n",
    "            \"num_molecules\": molecules_to_generate,\n",
    "            \"particles\": max(molecules_to_generate, particles),\n",
    "            \"scaled_radius\": sample_radius,\n",
    "        }\n",
    "\n",
    "    print(\"request\", payload)\n",
    "    session = requests.Session()\n",
    "\n",
    "    response = session.post(invoke_url, headers=headers, json=payload)\n",
    "    \n",
    "    response.raise_for_status()\n",
    "    response_body = response.json()\n",
    "    print(\"response\", response_body)\n",
    "    images = []\n",
    "    unique_smiles = set()\n",
    "    for entry in response_body.get('generated', []):\n",
    "        try:\n",
    "            if entry.get('smiles') not in unique_smiles:\n",
    "                unique_smiles.add(entry.get('smiles'))\n",
    "                molecule = Chem.MolFromSmiles(entry.get('smiles'))\n",
    "                images.append((Draw.MolToImage(molecule,  size=(600, 600)), f\"score: {entry.get('score')}\"))\n",
    "        except Exception as e:\n",
    "            print(f\"Couldn't process molecule {entry.get('smiles')}. Error: {e}\")\n",
    "    return images\n",
    "        \n",
    "    \n",
    "with gr.Blocks() as demo:\n",
    "    with gr.Accordion(label=\"Input\", open=True) as input:\n",
    "        smiles_dd = gr.Dropdown(\n",
    "            [(k,v) for k, v in smiles_examples.items()],\n",
    "            label=\"SMILES Examples\",\n",
    "            interactive=True,\n",
    "            container=True,\n",
    "            value=smiles_examples['Lisuride']\n",
    "        )\n",
    "    \n",
    "        smiles_string = gr.Textbox(\n",
    "            value=\"[H][C@@]12Cc3c[nH]c4cccc(C1=C[C@H](NC(=O)N(CC)CC)CN2C)c34\",\n",
    "            interactive=True,\n",
    "            label=\"SMILES String\",\n",
    "            container=True\n",
    "        )\n",
    "    \n",
    "        smiles_dd.change(fn=lambda selected: selected, inputs=[smiles_dd], outputs=[smiles_string], queue=False)\n",
    "        with gr.Row():\n",
    "            molecules_to_generate = gr.Number(\n",
    "                label=\"Number of molecules to generate\",\n",
    "                interactive=True,\n",
    "                minimum=1,\n",
    "                maximum=100,\n",
    "                value=30,\n",
    "            )\n",
    "    \n",
    "            particles = gr.Number(\n",
    "                minimum=3,\n",
    "                maximum=1000,\n",
    "                value=30,\n",
    "                interactive=True,\n",
    "                label=\"Number of particles\"\n",
    "            )\n",
    "\n",
    "        sampling_algorithm = gr.Dropdown(\n",
    "            [\n",
    "                \"CMA-ES Controlled Generation\",\n",
    "                \"Sampling Standard Deviation\"\n",
    "            ],\n",
    "            label=\"Sampling Algorithm\",\n",
    "            value=\"CMA-ES Controlled Generation\",\n",
    "            interactive=True\n",
    "        )\n",
    "\n",
    "        with gr.Group(visible=False) as sampling_group:\n",
    "            gr.Markdown(\"## Generation settings.\")\n",
    "            sample_radius = gr.Slider(\n",
    "                0, \n",
    "                2, \n",
    "                value=0.01,\n",
    "                step=0.01,\n",
    "                interactive=True,\n",
    "                label=\"Sample Radius\"\n",
    "            )\n",
    "        \n",
    "        with gr.Group(visible=True) as cma_group:\n",
    "            gr.Markdown(\"## Generation settings.\")\n",
    "            with gr.Row():\n",
    "                property_to_optimize = gr.Dropdown(\n",
    "                    [\"QED\", \"plogP\"],\n",
    "                    label=\"Property to Optimize\",\n",
    "                    interactive=True,\n",
    "                    value=\"QED\"\n",
    "                )\n",
    "                \n",
    "                maximize = gr.Checkbox(\n",
    "                    value=True,\n",
    "                    label=\"Maximize\",\n",
    "                    interactive=True\n",
    "                )\n",
    "\n",
    "            similarity_constraint = gr.Slider(\n",
    "                0, \n",
    "                1, \n",
    "                value=0.3,\n",
    "                label=\"Similarity Constraint\",\n",
    "                interactive=True\n",
    "            )\n",
    "\n",
    "            with gr.Row():\n",
    "                iterations = gr.Number(\n",
    "                    minimum=1,\n",
    "                    maximum=100,\n",
    "                    value=10,\n",
    "                    interactive=True,\n",
    "                    label=\"Number of iterations\"\n",
    "                )\n",
    "\n",
    "\n",
    "        sampling_algorithm.change(fn=lambda x: (gr.Group(visible=True), gr.Group(visible=False)) if x == \"CMA-ES Controlled Generation\" else (gr.Group(visible=False), gr.Group(visible=True)), inputs=[sampling_algorithm], outputs=[cma_group, sampling_group], queue=False)\n",
    "    with gr.Accordion(label=\"Output\", open=False) as output:\n",
    "        gallery = gr.Gallery(\n",
    "            label=\"Generated molecules\",\n",
    "            show_label=True,\n",
    "            object_fit=\"contain\",\n",
    "            height=\"auto\"\n",
    "        )\n",
    "            \n",
    "    with gr.Row() as buttons:\n",
    "        clear = gr.Button(\"Clear\")\n",
    "        btn = gr.Button(\"Run\")\n",
    "        \n",
    "        \n",
    "    btn.click(lambda: (gr.Accordion(open=False), gr.Accordion(open=True)), [], [input, output], queue=False).then(process_request,\n",
    "              inputs=[\n",
    "                  smiles_string,\n",
    "                  molecules_to_generate,\n",
    "                  sampling_algorithm,\n",
    "                  sample_radius,\n",
    "                  property_to_optimize,\n",
    "                  maximize,\n",
    "                  similarity_constraint,\n",
    "                  particles,\n",
    "                  iterations\n",
    "              ],\n",
    "              outputs=[\n",
    "                  gallery\n",
    "              ]\n",
    "             )\n",
    "    clear.click(lambda: (gr.Accordion(open=True), gr.Accordion(open=False)), [], [input, output], queue=False)\n",
    "    \n",
    "if __name__ == \"__main__\":\n",
    "    demo.launch(share=True)"
   ]
  }
 ],
